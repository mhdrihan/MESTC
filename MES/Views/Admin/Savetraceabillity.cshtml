@model MES.Models.traceabillitymodel

<div class="card mt-5">
    <div class="card-header bg-secondary text-white">Form Input Data</div>
    <div class="card-body">

        @using (Html.BeginForm("Savetraceabillity", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form-group">
                <label for="Serial_Number" class="form-label">Serial Number</label>
                @Html.TextBoxFor(m=>m.Serial_Number, new{@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Batch_ID" class="form-label">Batch ID</label>
                @Html.TextBoxFor(m=>m.Batch_ID, new {@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Refference_Name" class="form-label">Refference Name</label>
                @Html.TextBoxFor(m=>m.Refference_Name, new{@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Work_Order" class="form-label">Work Order</label>
                @Html.TextBoxFor(m=>m.Work_Order, new{@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Station_ID" class="form-label">Station ID</label>
                @Html.DropDownListFor(m=>m.Station_ID, ViewBag.stationID as SelectList, "Pilih Station", new {@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Station_Suffix" class="form-label">Station Suffix</label>
                @Html.TextBoxFor(m=>m.Station_Suffix, new{@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Station_Name" class="form-label">Station Name</label>
                @Html.TextBoxFor(m=>m.Station_Name, new{@class="form-control"})
            </div>
            <div class="form-group">
                <label for="User_ID" class="form-label">User ID</label>
                @Html.DropDownListFor(m=>m.User_ID, ViewBag.idUSer as SelectList, "Pilih ID User", new {@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Cavity_Number" class="form-label">Cavity Number</label>
                @Html.TextBoxFor(m=>m.Cavity_Number, new{@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Time_In" class="form-label">Time In</label>
                @Html.TextBoxFor(m=>m.Time_In, new{@class="form-control datepicker" , @type = "datetime-local"})
            </div>
            <div class="form-group">
                <label for="Time_Out" class="form-label">Time Out</label>
                @Html.TextBoxFor(m=>m.Time_Out, new{@class="form-control datepicker" , @type = "datetime-local"})
            </div>
            <div class="form-group">
                <label for="Status_Result" class="form-label">Status Result</label>
                @Html.DropDownListFor(m=>m.Status_Result, ViewBag.statusResult as SelectList, "Pilih Status", new {@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Status_Running" class="form-label">Status Running</label>
                @Html.DropDownListFor(m=>m.Status_Running, ViewBag.statusRunning as SelectList, "Pilih Status", new {@class="form-control"})
            </div>
            <div class="form-group">
                <label for="Transact_By" class="form-label">Transact By</label>
                @Html.TextBoxFor(m=>m.Transact_By, new{@class="form-control"})
            </div>
            <div class="form-group">
                <label for="FullRefference" class="form-label">Full Refference</label>
                @Html.TextBoxFor(m=>m.FullRefference, new{@class="form-control"})
            </div>

            <div class="modal-footer">
                <a href="/Admin/traceabillityAd" class="btn btn-secondary">Cancel</a>
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
        }
    </div>
</div>
